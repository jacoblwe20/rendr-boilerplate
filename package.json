{
  "name": "rendr-example",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-unit && npm run test-karma",
    "test-unit": "mocha --ui bdd --reporter spec ./test/app --recursive --require ./test/helper.js",
    "test-karma": "grunt compile && karma start test/karma.config.js --single-run",
    "start": "node index.js",
    "postinstall": "test -f ../../package.json && npm install ../../ || echo",
    "postupdate": "test -f ../../package.json && npm install ../../ || echo"
  },
  "dependencies": {
    "express": "~4.15.2",
    "underscore": "~1.8.3",
    "rendr-handlebars": "2.0.2",
    "rendr": "1.1.4",
    "debug": "~2.6.6",
    "request": "~2.81.0",
    "qs": "~6.4.0"
  },
  "devDependencies": {
    "grunt": "~1.0.1",
    "grunt-browserify": "~5.0.0",
    "grunt-contrib-stylus": "~1.2.0",
    "grunt-contrib-handlebars": "~1.0.0",
    "grunt-contrib-watch": "~1.0.0",
    "nodemon": "~1.11.0",
    "mocha": "~3.3.0",
    "chai": "~3.5.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-mocha": "~1.3.0",
    "grunt-contrib-compass": "~1.1.1"
  },
  "engines": {
    "node": ">=0.8"
  },
  "private": true
}
